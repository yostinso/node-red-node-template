{
    "name": "${fullPackageName}",
    "packageName": "${packageName}",
    "version": "0.0.1",
    "description": "A node to do some stuff",
    "author": "${githubUsername}",
    "homepage": "https://github.com/${githubUsername}/${fullPackageName}",
    "license": "Apache-2.0",
    "maintainers": [
        "${maintainer}"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/${githubUsername}/${fullPackageName}.git"
    },
    "bugs": {
        "url": "https://github.com/${githubUsername}/${fullPackageName}/issues"
    },
    "keywords": [
        "node-red",
        "node-red-contrib",
        "contrib",
        "smart-home",
        "home-automation"
    ],
    "node-red": {
        "nodes": {}
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "compile:ts": "tsc",
        "compile:compilers": "tsc --project ./compile",
        "compile:views_ts": "tsc --project ./${packageName}/views",
        "compile:views": "node ./compile/js/compileViews.js --outDir dist --locales ./${packageName}/locales ./${packageName}/views",
        "compile:clean": "rm -rf dist compile/js test/views/*.js",
        "compile:all": "npm run compile:compilers && npm run compile:views_ts && npm run compile:views && npm run compile:ts"
    },
    "devDependencies": {
        "@types/async": "^3.2.13",
        "@types/jest": "^27.5.1",
        "@types/node": "^17.0.35",
        "@types/node-red": "^1.2.1",
        "jest": "^28.1.0",
        "node-red": "^2.2.2",
        "ts-jest": "^28.0.2",
        "typescript": "^4.6.4"
    },
    "dependencies": {
        "async": "^3.2.3",
        "node-red-es6-shim": "^1.0.7"
    }
}
