{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "typescript",
			"tsconfig": "tsconfig.json",
			"problemMatcher": [
				"$tsc"
			],
			"group": {
				"kind": "build"
			},
			"label": "tsc: build - tsconfig.json"
		},
		{
			"type": "shell",
			"command": "chmod",
			"args": [
				"a+x",
				"${workspaceFolder}/dist/generate.js"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"dependsOn": [
				"tsc: build - tsconfig.json"
			],
			"label": "Make generate.js executable"
		}
	]
}